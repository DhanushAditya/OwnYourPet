<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Petscare"
        tools:targetApi="31">
        <activity android:name=".home"
            android:theme="@style/Theme.AppCompat">
        </activity>
        <activity android:name=".chatbot"
            android:theme="@style/Theme.AppCompat">
        </activity>
        <activity android:name=".chatusers"
            android:theme="@style/Theme.AppCompat">
        </activity>
        <activity android:name=".ChatPage"
            android:theme="@style/Theme.AppCompat">
        </activity>
        <activity android:name=".Myads"
            android:theme="@style/Theme.AppCompat">
        </activity>
        <activity android:name=".adsForm"
            android:theme="@style/Theme.AppCompat">
        </activity>
        <activity android:name=".getProducts"
            android:theme="@style/Theme.AppCompat">
        </activity>
        <activity android:name=".dashboard"
            android:theme="@style/Theme.AppCompat">
        </activity>
        <activity android:name=".prodForm"
            android:theme="@style/Theme.AppCompat">
        </activity>
        <activity android:name=".stockList"
            android:theme="@style/Theme.AppCompat">
        </activity>
        <activity android:name=".projectView"
            android:theme="@style/Theme.AppCompat">
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"


            android:theme="@style/Theme.Petscare">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>